{
  // Configuration for 3suite-maestro
  orchestratorConfigFile: "config.json5",
  devDependenciesLocation: "orchestrator-deps",
  
  // Configuration for 3lib-orchestrator
  processes: [
    {
      name: "httpServer",
      source: "3sig/3suite-http-server",
      sourceActions: [{ type: "chmod" }],
      config: {port: 4004, directory: "node_modules"},
    },
    {
      name: "browser",
      source: "3sig/3suite-browser",
      sourceActions: [
        {
          type: "unzip"
        }
      ],
      sourceExecOverride: "./3suite-browser.app/Contents/MacOS/3suite-browser",
      loadConfig: "browser",
    },
  ],

  configs: {
    browser: {
      url: "http://localhost:4004/3lib-config/main.js",
      fullscreen: false,
      disableCursor: false,

      windowWidth: 800,
      windowHeight: 600,
    }
  }
}
